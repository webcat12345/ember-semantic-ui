<div class="ui padded grid">
    <div class="padded row explore--analyze-head">
        <div class="nine wide column">
            <div class="ui input fluid">

            </div>
        </div>
        <div class="seven wide column">
          {{#if canCompare}}
              <hr>
          {{/if}}
        </div>
    </div>
  {{#if bpt.name}}
      <div class="padded row explore--main-row">
          <div class="sixteen wide column">
              Profile:
            {{#if bpt.name}}
                <button type="button"
                        class="small ui basic button" {{action "remove_point" "Bpt"}}> {{bpt.name}}
                    <b>X</b></button>
            {{else}}
                <button type="button" class="small ui inline basic button disabled"> Select Point A</button>
            {{/if}}
          </div>
      </div>
      <div class="padded row explore--main-row">
          <div class="sixteen wide column">
              Against:
            {{#if apt.name}}
                <button type="button"
                        class="small ui basic button" {{action "remove_point" "Apt"}}> {{apt.name}}
                    <b>X</b></button>
            {{else}}
                <button type="button" class="small ui inline basic button disabled"> Select Baseline</button>
            {{/if}}
              |
            {{#each behavior in possible_behavior}}
                <button type="button"
                        class="small ui inline basic button" {{action "behavior" behavior}}> {{behavior.display}}
                </button>
            {{/each}}
          </div>
      </div>
      <div>
        {{#if canCompare}}
            <button {{action "send_comp" dataset._id compareObject apt.value bpt.value bpt.filter}}
                    class="tiny ui primary button">
              {{#if loading}}
                {{fa-icon icon="refresh" spin=true size="1"}}
              {{else}}
                  Go
              {{/if}}
            </button>
        {{/if}}
      </div>
  {{else}}
      <div class="padded row explore--main-row">
          <div class="sixteen wide column">
              Profile
              <button type="button" class="small ui green button" {{action 'start_compare'}}>+
              </button>
          </div>
      </div>
  {{/if}}
</div>
