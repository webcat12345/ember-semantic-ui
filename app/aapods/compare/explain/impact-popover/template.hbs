{{#bootstrap-popover title="More Details"
linktext=(number-fmt 'changepercunit' point.facet.vsnochangeperc)}}
    <table class="table table-bordered table-in-popover">
        <thead>
        <tr>
            <th></th>
            <th></th>
            <th>
              {{#if model.results.apt.bucket.[0].value }}
                {{#if model.results.apt.bucket.[1] }}
                  {{#each model.results.apt.bucket as |one_value|}}
                    {{one_value.name}}{{one_value.op}}{{one_value.value}}
                      <br>
                  {{/each}}
                {{else}}
                  {{#if model.results.apt.bucket.[0] }}
                    {{model.results.apt.bucket.[0].value}}
                  {{/if}}
                {{/if}}
              {{else}}
                {{model.results.apt.bucket}}
              {{/if}}
            </th>

            <th colspan="2" class="text-center">
              {{#if model.results.bpt.bucket.[0].value }}
                {{#if model.results.bpt.bucket.[1] }}
                  {{#each model.results.bpt.bucket as |one_value|}}
                    {{one_value.name}}{{one_value.op}}{{one_value.value}}
                      <br>
                  {{/each}}
                {{else}}
                  {{#if model.results.bpt.bucket.[0] }}
                    {{model.results.bpt.bucket.[0].value}}
                  {{/if}}
                {{/if}}
              {{else}}
                {{model.results.bpt.bucket}}
              {{/if}}
            </th>

        </tr>
        <tr>
            <th></th>
            <th></th>
            <th></th>
            <th>Expected</th>
            <th>Actual</th>
        </tr>
        </thead>
        <tbody>
        <tr class="info">
            <td><strong>Overall</strong></td>
            <td><strong>{{model.metric.name}}</strong></td>
            <td>
              <strong>{{number-fmt 'metric' model.results.apt.om}}</strong>{{model.metric.unit_type}}
            </td>
            <td>
                <strong> {{number-fmt 'metric' point.facet.nochange.p_om}}</strong>{{model.metric.unit_type}}
            </td>
            <td>
                <strong>{{number-fmt 'metric' model.results.bpt.om}}</strong>{{model.metric.unit_type}}
            </td>
        </tr>
        {{#if model.metric.calculation.numerator}}
            <tr>
                <td></td>
                <td>{{model.metric.calculation.numerator.name}}</td>
                <td>
                  {{number-fmt 'metric' model.results.apt.mct}}{{model.metric.calculation.numerator.unit_type}}
                </td>
                <td>
                  {{number-fmt 'metric' point.facet.nochange.p_mct}}{{model.metric.calculation.numerator.unit_type}}
                </td>
                <td>
                  {{number-fmt 'metric' model.results.bpt.mct}}{{model.metric.calculation.numerator.unit_type}}
                </td>
            </tr>

        {{/if}}
        {{#if model.metric.calculation.denominator}}
            <tr>
                <td></td>
                <td>{{model.metric.calculation.denominator.name}}</td>
                <td>
                  {{number-fmt 'metric' model.results.apt.ct}}{{model.metric.calculation.denominator.unit_type}}
                </td>
                <td>
                  {{number-fmt 'metric' model.results.bpt.ct}}{{model.metric.calculation.denominator.unit_type}}
                </td>
                <td>
                  {{number-fmt 'metric' model.results.bpt.ct}}{{model.metric.calculation.denominator.unit_type}}
                </td>
            </tr>
        {{/if}}
        <tr class="info">
            <td><strong>This Segment</strong></td>
            <td><strong>{{model.metric.name}}</strong></td>
            <td>
                <strong>{{number-fmt 'metric' point.facet.facet_data.a.om}}</strong>{{model.metric.unit_type}}
            </td>
            <td>
                <strong>{{number-fmt 'metric' point.facet.nochange.om}}</strong>{{model.metric.unit_type}}
            </td>
            <td>
                <strong>{{number-fmt 'metric' point.facet.facet_data.b.om}}</strong>{{model.metric.unit_type}}
            </td>
        </tr>
        {{#if model.metric.calculation.numerator}}
            <tr>
                <td></td>
                <td>{{model.metric.calculation.numerator.name}}</td>
                <td>
                  {{number-fmt 'metric' point.facet.facet_data.a.mct}}{{model.metric.calculation.numerator.unit_type}}
                </td>
                <td>
                  {{number-fmt 'metric' point.facet.nochange.mct}}{{model.metric.calculation.numerator.unit_type}}
                </td>
                <td>
                  {{number-fmt 'metric' point.facet.facet_data.b.mct}}{{model.metric.calculation.numerator.unit_type}}
                </td>
            </tr>
        {{/if}}
        {{#if model.metric.calculation.denominator}}
            <tr>
                <td></td>
                <td>{{model.metric.calculation.denominator.name}}</td>
                <td>
                  {{number-fmt 'metric' point.facet.facet_data.a.ct}}{{model.metric.calculation.denominator.unit_type}}
                </td>
                <td>
                  {{number-fmt 'metric' point.facet.facet_data.b.ct}}{{model.metric.calculation.denominator.unit_type}}
                </td>
                <td>
                  {{number-fmt 'metric' point.facet.facet_data.b.ct}}{{model.metric.calculation.denominator.unit_type}}
                </td>
            </tr>
        {{/if}}
        </tbody>
    </table>

    <table class="table table-striped table-in-popover">
        <thead>
        <tr>
            <th>Overall Change</th>
            <th>Actual</th>
            <th>Expected</th>
            <th>Impact</th>
            <th>% Impact</th>
        </tr>
        </thead>
        <tbody>

        <tr>
            <td>{{model.metric.name}}</td>
            <td>{{number-fmt 'metric' model.results.d.om}}</td>
            <td>{{number-fmt 'metric' point.facet.nochange.diff}}</td>
            <td>{{number-fmt 'metric' point.facet.vsnochangediff}}</td>
            <td class={{point.vsnochangecolor}}>
              {{number-fmt 'changepercunit' point.facet.vsnochangeperc}}
            </td>
        </tr>

        </tbody>
    </table>

{{/bootstrap-popover}}